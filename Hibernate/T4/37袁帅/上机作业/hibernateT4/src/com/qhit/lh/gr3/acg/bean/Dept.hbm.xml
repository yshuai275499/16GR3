<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.qhit.lh.gr3.acg.bean.Dept" table="t_dept">
		<id name="deptId" column="deptId" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="dname" column="dname" type="string"></property>
		
		<set name="emps" cascade="save-update">
			<key column="deptId"></key>
			<one-to-many class="com.qhit.lh.gr3.acg.bean.Emp"/>
		</set>
	</class>
</hibernate-mapping>
