<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.qhit.lh.gr3.acg.bean.Emp" table="t_emp" >
		<id name="eid" column="eid" type="integer" >
			<generator class="native"></generator>
		</id>
		
		<property name="ename" column="ename" type="string"></property>
		<property name="sex" column="sex" type="string"></property>
		<property name="deptId" column="deptId" type="string"></property>
		<!-- 一对一关系映射 -->
		<one-to-one name="user" class="com.qhit.lh.gr3.acg.bean.User" cascade="all"></one-to-one>
		<!-- 多对一关系映射 -->
		<many-to-one name="dept" class="com.qhit.lh.gr3.acg.bean.Dept" column="deptId" lazy="false"></many-to-one>
		
	</class>
</hibernate-mapping>
