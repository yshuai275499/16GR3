<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
 <class name="om.qhit.lh.gr3.acg.t2.bean.Dept" table="dept">
    <id name="oid" column="oid" type="integer">
       <generator class="sequence">
         <param name="sequence">dept</param>
       </generator>
    </id>
    <property name="dname" column="dname" type="string"></property>
    <property name="address" column="dname" type="string"></property>
 </class>
</hibernate-mapping>
